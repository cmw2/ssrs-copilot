{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureOpenAI": {
    "Endpoint": "https://your-azure-openai-resource.openai.azure.com/",
    "ApiKey": "your-api-key",
    "DeploymentName": "gpt-4",
    "Temperature": 0.0,
    "SystemMessagePath": "Prompts/SystemMessage.txt"
  },
  "AzureSearch": {
    "Endpoint": "https://your-search-service.search.windows.net",
    "ApiKey": "your-search-api-key",
    "IndexName": "psdc-reports",
    "SemanticConfigurationName": "azureml-default",
    "VectorSearchEnabled": true,
    "EmbeddingDeploymentName": "text-embedding-3-large",
    "FieldMapping": {
      "IdField": "id",
      "TitleField": "title",
      "ContentField": "content",
      "UrlField": "url",
      "FilePathField": "filepath",
      "MetadataField": "meta_json_string",
      "VectorField": "contentVector",
      "ParentIdField": "parent_id"
    }
  },
  "Ssrs": {
    "ApiBaseUrl": "https://your-ssrs-server/api/v2.0",
    "ReportViewerUrl": "https://your-ssrs-server/reports/report-viewer",
    "Username": "", 
    "Password": "",
    "Domain": ""
  }
}
